{
  "contextFileName": ["GEMINI-plan.md", "AGENTS.md", "SPEC.md", "TESTING.md"],
  "urlTemplate": "https://github.com/blockscout/mcp-server/issues/new?title={title}&body={info}",
  "checkpointing": {
    "enabled": true
  },
  "theme": "ANSI",
  "hideBanner": true,
  "mcpServers": {
    "codealive": {
      "httpUrl": "https://mcp.codealive.ai/api/",
      "headers": {
        "Authorization": "Bearer ${CODEALIVE_API_KEY}"
      },
      "timeout": 240000
    }
  }
}
